{
  "name": "cwayassistant",
  "version": "0.1.0",
  "description": "Google Chat HTTP function for cwayassistant",
  "main": "index.js",
  "scripts": {
    "test": "mocha -p -j 2 -t 5s \"test/**/*.test.js\"",
    "test:coverage": "nyc npm test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "echo 'JSDoc type checking enabled via jsconfig.json checkJs mode'",
    "dev": "functions-framework --target=app --port=8080"
  },
  "keywords": [
    "Google Chat",
    "HTTP Function"
  ],
  "author": "Hugo Borba",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "@googleapis/chat": "^22.0.0",
    "google-auth-library": "^9.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "chai": "^4.3.10",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "proxyquire": "^2.1.3",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.3.4"
  }
}
